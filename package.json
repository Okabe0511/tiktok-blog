{
  "name": "vue-express-ssr-blog",
  "version": "1.0.0",
  "scripts": {
    "dev": "node server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.js",
    "serve": "cross-env NODE_ENV=production node server.js"
  },
  "dependencies": {
    "ant-design-vue": "^4.2.6",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "marked": "^17.0.1",
    "multer": "^2.0.2",
    "mysql2": "^3.6.0",
    "sequelize": "^6.32.1",
    "sirv": "^2.0.3",
    "sqlite3": "^5.1.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "vite": "^4.4.9"
  },
  "type": "module"
}
